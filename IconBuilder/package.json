{
  "name": "icon-builder",
  "version": "1.1.0",
  "description": "The Icon builder built the svg icon as a react component.",
  "scripts": {
    "generateUuId": "ts-node ./tools/generateUuId.ts",
    "build": "echo \"Icon builder is running!\"",
    "compile": "ts-node ./compile-cmd.ts",
    "append": "ts-node ./append-to-release-cmd.ts",
    "new": "ts-node ./new-release-cmd.ts",
    
    "appendToRelease" : "npm-run-all -s compile append",
    "createNewRelease": "npm-run-all -s compile new",
    "test": "ts-node ./preprocess.ts",

    "createIconRelease": "npm run compile iconlib && npm run new iconlib",
    "createNavIconRelease": "npm run compile naviconlib && npm run new naviconlib"
  },
  "author": "usue",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@types/gulp": "^4.0.13",
    "@types/gulp-less": "^0.0.33",
    "@types/gulp-minify-css": "^0.0.33",
    "@types/gulp-rename": "^2.0.2",
    "@types/merge2": "^1.4.1",
    "@types/node": "^20.4.9",
    "@types/uuid": "^9.0.2",
    "@types/xml2js": "^0.4.11",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-less": "^5.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "merge2": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@icon-park/compiler": "^1.0.7",
    "@types/react": "^18.2.20",
    "del": "^6.0.0",
    "mkdirp": "^3.0.1",
    "svgo": "^3.0.2",
    "uuid": "^9.0.0",
    "xml2js": "^0.6.2"
  }
}
