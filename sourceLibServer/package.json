{
    "name": "ultrasource",
    "private": true,
    "version": "1.0.0",
    "main": "dist/src1/server.js",
    "type": "module",
    "scripts": {
        "watch-server": "nodemon --ignore public/ --ignore logs/ --ignore dist/ --watch src -e ts,tsx --exec 'npm run build && npm start'",
        "dev": "cross-env NODE_ENV=development nodemon --ignore public/ --ignore logs/ --watch src -e ts,tsx --exec ts-node src/server.ts",
        "resetFolder": "rimraf dist/*",
        "moveStatic": "shx cp -r log dist/ && shx cp -r source dist/ && shx cp -r node_modules dist/ && shx cp -r ssl dist/ && shx cp -r package.json dist/ && shx cp -r types dist/ ",
        "compile": "tsc",
        "build": "npm-run-all resetFolder compile moveStatic",
        "cmdStart": "cross-env NODE_ENV=production node server.js",
        "start": "nohup cross-env NODE_ENV=production node server.js &"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.22.10",
        "@babel/preset-react": "^7.22.5",
        "@types/gulp-less": "^0.0.33",
        "@types/gulp-minify-css": "^0.0.33",
        "@types/gulp-rename": "^2.0.2",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/koa": "^2.13.6",
        "@types/koa__cors": "^4.0.0",
        "@types/koa-router": "^7.4.4",
        "@types/koa-session": "^6.4.1",
        "@types/koa-static": "^4.0.2",
        "@types/merge2": "^1.4.1",
        "@types/node": "^20.4.2",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-react": "^7.32.2",
        "mkdirp": "^3.0.1",
        "nodemon": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "@babel/core": "^7.22.10",
        "@icon-park/compiler": "^1.0.7",
        "@koa/cors": "^4.0.0",
        "@types/gulp": "^4.0.13",
        "@types/lodash": "^4.14.195",
        "@types/react": "^18.2.19",
        "dotenv": "^16.3.1",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-cli": "^2.3.0",
        "gulp-less": "^5.0.0",
        "gulp-minify-css": "^1.2.4",
        "gulp-rename": "^2.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "https": "^1.0.0",
        "jsonwebtoken": "^9.0.1",
        "koa": "^2.14.2",
        "koa-body": "^6.0.1",
        "koa-jwt": "^4.0.4",
        "koa-router": "^12.0.0",
        "koa-session": "^6.4.0",
        "koa-static": "^5.0.0",
        "koa2-cors": "^2.0.6",
        "lodash": "^4.17.21",
        "lowdb": "^6.0.1",
        "merge2": "^1.4.1",
